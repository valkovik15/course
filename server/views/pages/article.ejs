<!DOCTYPE html>
<html lang="en" ng-app="MyApp" ng-controller="AppCtrl">

<head>
    <meta charset="UTF-8">
    <title>{{title}}</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,400italic">

    <link rel='stylesheet' href='https://cdn.gitcdn.link/cdn/angular/bower-material/v1.1.10/angular-material.css'>
    <link rel='stylesheet' href='https://material.angularjs.org/1.1.10/docs.css'>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css">

    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css'>
    <link rel="stylesheet" href="css/style.css">
    <script src="https://js.pusher.com/4.3/pusher.min.js"></script>

    <% include ../partials/stylesheet %>
</head>

<body>
<style>

</style>
<% include ../partials/header %>
<div  layout="column" style="height:500px;" ng-cloak="" class="sidenavdemoBasicUsage"  ng-init="postid='<%= id %>'; user='<%= user %>'">

    <style>
       section {margin-top:70px}
        .btn-1 {text-transform: none}
    </style>
    <section layout="row" flex="" layout-align>

        <md-sidenav class="md-sidenav-left" md-component-id="left" md-is-locked-open="$mdMedia('gt-md')" md-whiteframe="4">

            <md-toolbar class="deep-orange">
                <h1 >Wiki</h1>
                <h4  align="center" >{{title}}</h4>
                <md-button    class="btn-1" ng-repeat="x in toc" ng-click="change(x)" ng-bind-html="x.slice(x.indexOf('>')+1, x.lastIndexOf('<'))"></md-button>
                <div  ng-controller="RatingController as ratingForm"  class="rating-module"  ng-init="postid='<%= id %>'; user='<%= user %>'">
                    <div class="stage1"  name="frmRatingStep1">
                        <directive star-rating class="holder border-block"  ng-model="ratingForm.starrate" max="5" ng-click="update('rating'+ratingForm.starrate)"></directive>
                        <md-button ng-click="rank();">Leave your rank!</md-button>
                    </div>
                </div>
            </md-toolbar>
            <md-content layout-padding="" ng-controller="LeftCtrl">
                <md-button ng-click="close()" class="md-primary" hide-gt-md="">
                    Close Sidenav Left
                </md-button>

            </md-content>

        </md-sidenav>
                <md-content flex="" layout-padding="">

            <div layout="column" layout-align="top center">
                <md-card>

                    <md-card-title>
                        <md-card-title-text>
                            <span class="md-headline" ng-bind-html="toc[index]">  </span>
                        </md-card-title-text>
                    </md-card-title>
                    <md-card-content>
                       <div ng-bind-html="steps[index]">
                       </div>

                    </md-card-content>
                    <md-card-actions layout="row" layout-align="end center">
                        <md-button ng-click="sub()" ng-show="index">Previous step</md-button>
                        <md-button ng-click="add()" ng-show="steps.length-index-1">Next step</md-button>
                    </md-card-actions>
                </md-card>

                <div>
                    <md-button ng-click="toggleRight()" ng-hide="isOpenRight()" class="md-primary">
                        Toggle right
                    </md-button>
                </div>
            </div>

            <div flex=""></div>

        </md-content>

        <md-sidenav class="md-sidenav-right md-whiteframe-4dp" md-component-id="right">

            <md-toolbar class="md-theme-light">
                <h1 class="md-toolbar-tools">Sidenav Right</h1>
                <div class="md-toolbar-tools">
                    <span>Normal</span>
                </div>
            </md-toolbar>
            <md-content ng-controller="RightCtrl"  layout-padding="" id="wrap">
                <form>
                    <md-input-container>
                        <label for="testInput">Test input</label>
                        <input id="testInput" ng-model="data" md-autofocus="" type="text">
                    </md-input-container>
                    <md-button ng-click="submit()" class="md-primary">
                        Submit comment
                    </md-button>
                </form>
                <md-list flex="" ng-model="list">
                    <md-divider></md-divider>
                    <md-subheader class="md-no-sticky">Comments</md-subheader>
                    <md-list-item class="md-3-line" ng-repeat="item in todos">
                        <img ng-src="{{item.face}}?20" class="md-avatar" alt="{{todos[0].who}}">
                        <div class="md-list-item-text">
                            <h3>Я люблю тебя</h3>
                            <p>{{item.text}}</p>
                            <p>Это навечно</p>
                            <div class="md-toolbar-tools">

                                <span flex></span>
                                <md-button class="md-icon-button">
                                    <md-icon md-font-icon=" fa-heart-o" class="fa"></md-icon>
                                </md-button>
                            </div>
                        </div>
                    </md-list-item>


                </md-list>
                <md-button ng-click="close()" class="md-primary">
                    Close Sidenav Right
                </md-button>
            </md-content>

        </md-sidenav>

    </section>

</div>

